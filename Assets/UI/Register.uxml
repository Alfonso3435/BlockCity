<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/Register.uss?fileID=7433441132597879392&amp;guid=f5375c1bfe2830f4990bb72a1b34ffee&amp;type=3#Register" />
    <engine:VisualElement name="Contenedor" style="flex-grow: 1; justify-content: flex-start; align-items: center; left: auto; align-self: auto;">
        <engine:Button name="Regreso" class="botonregreso" style="border-top-width: 0; border-right-width: 0; border-bottom-width: 0; border-left-width: 0;" />
        <engine:Label text="Register&#10;" name="Titulo" class="titulo" />
        <engine:VisualElement name="Formulario" class="contenedorFormulario" style="flex-grow: 1;">
            <engine:VisualElement name="Usuario" class="contenedorUsuario" style="flex-grow: 1; max-height: 42%;">
                <engine:TextField placeholder-text="Email" name="Correo" label="Email    ã…¤" max-length="-1" class="textfield" />
                <engine:TextField placeholder-text="Name" name="Nombre" label="Full Name" class="textfield" />
                <engine:TextField placeholder-text="User Name" name="NombreUsuario" label="User Name" class="textfield" />
                <engine:DropdownField label="Nationality" name="Nacionalidad" choices="Mexico,United States" class="nacionalidad" />
