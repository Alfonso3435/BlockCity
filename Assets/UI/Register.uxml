        </engine:VisualElement>
        <engine:Button name="ErrorPopUp" style="position: absolute; width: 400px; height: 400px; background-image: url(&quot;project://database/Assets/Layer%20Lab/GUI-SimpleRound/ResourcesData/Sprites/Components/Popup/Popup02.png?fileID=2800000&amp;guid=a778b1b935d9346a58cb4e03ec4ca19f&amp;type=3#Popup02&quot;); background-color: rgba(188, 188, 188, 0); border-top-width: 0; border-right-width: 0; border-bottom-width: 0; border-left-width: 0; top: 30%; left: 75%;">
